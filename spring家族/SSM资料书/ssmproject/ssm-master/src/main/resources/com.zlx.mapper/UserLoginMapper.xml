<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><!--mapper文件约束头-->
<mapper namespace="com.dao.UserLoginDao">
    <select id="findUser" resultType="com.po.UserLogin" parameterType="com.po.UserLogin">
        select * from userlogin where uname=#{uname} and password=#{password}
    </select>
</mapper>